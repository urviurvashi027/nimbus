{
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "develpment": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "beta": {
      "developmentClient": true
    },
    "production": {
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "package": "com.silentBonus.app",
        "buildType": "release",
        "proguardRules": "proguard-rules.pro"
      },
      "distribution": "store",
      "credentialsSource": "remote"
    }
  }
}
