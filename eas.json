{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "develpment": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "beta": {
      "developmentClient": true
    },
    "production": {
      "ios": {},
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "distribution": "store",
      "credentialsSource": "remote"
    }
  }
}
